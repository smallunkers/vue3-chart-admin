.d3-bar-chart
  width 100%
  height 400px
  .y-axis, .x-axis
    .dotted-grid
      stroke #DCDCDC
      shape-rendering crispEdges
      stroke-dasharray 5
  .tooltip
    font-family simsun
    width 120px
    height auto
    background-color rgba(0, 0, 0, 0.6)
    border-radius 4px
    color white
    .text
      margin-left 6px
